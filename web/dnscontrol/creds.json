{
    "DoH": {
        "TYPE": "DNSOVERHTTPS",
        "host": "cloudflare-dns.com",
    },
    "knot": {
        "TYPE": "AXFRDDNS",
        "master": "host.containers.internal:53",
        "update-mode": "tcp",
        "transfer-mode": "tcp",
        "transfer-key": "$DNSCONTROL_TSIG",
        "update-key": "$DNSCONTROL_TSIG",
    },
    "cloudflare": {
        "TYPE": "CLOUDFLAREAPI",
        "accountid": "$DNSCONTROL_CLOUDFLARE_ACCOUNT_ID",
        "apitoken": "$DNSCONTROL_CLOUDFLARE_API_TOKEN",
    },
}
